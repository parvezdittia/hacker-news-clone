{
  "hosting": {
    "public": "src",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "helloWorld"
      }
    ]
  },
  "functions":{
    "source": "functions",
    "runtime": "nodejs8"
  }
}
